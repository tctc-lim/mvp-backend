{
    "version": 2,
    "git": {
        "deploymentEnabled": {
            "main": true,
            "dev": true,
            "feature/*": false
        }
    },
    "builds": [
        {
            "src": "src/main.ts",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "src/main.ts",
            "methods": [
                "GET",
                "POST",
                "PUT",
                "DELETE",
                "PATCH",
                "OPTIONS"
            ]
        },
        {
            "src": "/(.*)",
            "dest": "src/main.ts"
        }
    ]
}